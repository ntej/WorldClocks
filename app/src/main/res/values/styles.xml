<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@android:color/background_light</item>
    </style>

    <style name="MaterialClone" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimary">@color/colorPrimaryM</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkM</item>
        <item name="colorAccent">@color/colorAccentM</item>
        <item name="android:windowBackground">@color/colorBackgroundM</item>
    </style>



</resources>
